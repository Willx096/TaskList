tareas\">\n    <div v-if=\"!tareas.length\" class=\"alert alert-info\" role=\"alert\">\n        No se han agregado personas\n    </div>\n    <table class=\"table\">\n        <thead>\n            <tr>\n                <th>Lista</th>\n                <!--<th>Apellido</th>\n                <th>Email</th>-->\n                <th>Acciones</th>\n            </tr>\n        </thead>\n        <tbody>\n            <tr v-for=\"lista in listas\" :key=\"lista.id\">\n                <td v-if=\"editando === lista.id\">\n                    <input type=\"text\" class=\"form-control\" v-model=\"lista.num\" />\n                </td>\n                <td v-else>\n                    {{ lista.num}}\n                </td>\n                <!--<td v-if=\"editando === persona.id\">\n                    <input type=\"text\" class=\"form-control\" v-model=\"persona.apellido\" />\n                </td>-->\n                <!--<td v-else>\n                    {{ persona.apellido}}\n                </td>-->\n                <!--<td v-if=\"editando === persona.id\">\n                    <input type=\"email\" class=\"form-control\" v-model=\"persona.email\" />\n                </td>-->\n                <!--<td v-else>\n                    {{ persona.email}}\n                </td>-->\n                <td v-if=\"editando === lista.id\">\n                    <button class=\"btn btn-success\" @click=\"guardarLista(lista)\">üíæ Guardar</button>\n                    <button class=\"btn btn-secondary ml-2\" @click=\"cancelarEdicion(lista)\">‚ùå Cancelar</button>\n                </td>\n                <td v-else>\n                    <button class=\"btn btn-info\" @click=\"editarLista(lista)\">‚úèÔ∏è Editar</button>\n                    <button class=\"btn btn-danger ml-2\" @click=\"$emit('delete-persona', lista.id)\">üóëÔ∏è Eliminar</button>\n                </td>\n            </tr>\n        </tbody>\n    </table>\n</div>\n</template>\n\n<script>\nexport default {\n    name: 'lista-tareas',\n    props: {\n        personas: Array,\n    },\n    data() {\n        return {\n            editando: null,\n        }\n    },\n    methods: {\n        editarLista(lista) {\n            this.listaEditada = Object.assign({}, lista);\n            this.editando = lista.id;\n        },\n        guardarLista(lista) {\n            if (!lista.length) {\n                return;\n            }\n            this.$emit('actualizar-lista', lista.id, lista);\n            this.editando = null;\n        },\n        cancelarEdicion(lista) {\n            Object.assign(lista, this.listaEditada);\n            this.editando = null;\n        }\n    }\n}\n</script>\n\n<style scoped>\n</style>\n"],"sourceRoot":"src/components"}]}